@model projet_dotnet.Models.Pfe_Etudiant

@{
    ViewData["Title"] = "Create";
}

<div class="form-body">
    <div class="row">
        <div class="form-holder">
            <div class="form-content">
                <div class="form-items">
                    <h3>Ajouter Pfe_Etudiant</h3>
                    <form asp-action="Create">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                        <div class="col-md-12">
                            <label asp-for="PfeId" class="control-label"></label>
                            <select asp-for="PfeId" class ="form-control" asp-items="ViewBag.PfeId"></select>
                        </div>


                       <div class="col-md-12">
                        <label asp-for="EtudiantId" class="control-label"></label>
                        <select asp-for="EtudiantId" class ="form-control" asp-items="ViewBag.EtudiantId"></select>
                       </div>
                       

                        <div class="form-button mt-3">
                            <input type="submit" value="Create" class="btn btn-warning" />
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<div>
    <button type="button" class="btn btn-outline-warning"><a asp-action="Index">Back To List</a></button>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
